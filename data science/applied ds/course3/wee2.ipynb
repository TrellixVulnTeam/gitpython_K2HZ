    "import string\n",
    "\n",
    "def string_processing(string_list):\n",
    "    punctuations=list(string.punctuation)\n",
    "    t=(\" \").join(string_list).lower()\n",
    "    print(t)\n",
    "    for x in t : \n",
    "        if x in punctuations: \n",
    "            t = t.replace(x, \"\")\n",
    "    return t\n",
    "print(string_processing(['hello,', 'world!']))"
   ]
